# 开发环境配置 - 默认端口 3000
# 复制此文件为 .env.local 并修改配置

# 应用配置
NEXT_PUBLIC_APP_NAME="AppStore 评论分析系统"
NEXT_PUBLIC_APP_PORT=3000

# Moonshot Kimi API 配置（必需）
# 从 https://platform.moonshot.cn/ 获取 API Key
MOONSHOT_API_KEY=your_moonshot_api_key_here
MOONSHOT_BASE_URL=https://api.moonshot.cn/v1

# 数据存储配置（可选，默认使用本地 JSON 文件）
STORAGE_TYPE=local

# Vercel KV 配置（生产环境）
# KV_URL=your_vercel_kv_url
# KV_REST_API_URL=your_vercel_kv_rest_api_url
# KV_REST_API_TOKEN=your_vercel_kv_rest_api_token
# KV_REST_API_READ_ONLY_TOKEN=your_vercel_kv_read_only_token

# Supabase 配置（可选）
# SUPABASE_URL=your_supabase_url
# SUPABASE_ANON_KEY=your_supabase_anon_key
